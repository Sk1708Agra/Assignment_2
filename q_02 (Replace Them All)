import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        long x = sc.nextLong();
        String str = Long.toString(x);
        int len = str.length();
        String str1 = "";

        for (int i=0; i<len; i++){
            if(str.charAt(i) == '0')
            str1 = str1 + '5';
            else
                str1 = str1+str.charAt(i);
        }
        System.out.println(str1);
    }
}
